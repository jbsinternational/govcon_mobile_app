<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
            xmlns:gap   = "http://phonegap.com/ns/1.0"
            id          = "com.phonegap.example"
            versionCode = "10"
            version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>GovCon 2015 Schedule</name>

    <description>
        The Schedule for the GovCon 2015
    </description>

    <author href="https://jbsinternational.com" email="webdev@jbsinternational.com">
        JBS International - Webdev Team
    </author>

    <gap:platform name="ios" />
    <gap:platform name="android" />
    <gap:platform name="webos" />
    <gap:platform name="symbian" />
    <gap:platform name="blackberry" />
    <gap:platform name="winphone" />

    <icon src="icons/ios/icon_57.png" gap:platform="ios" width="57" height="57" />
    <icon src="icons/ios/icon_72.png" gap:platform="ios" width="72" height="72" />
    <icon src="icons/ios/icon_114.png" gap:platform="ios" width="114" height="114" />
    <icon src="icons/ios/icon_144.png" gap:platform="ios" width="144" height="144" />

    <icon src="icons/android/icon_ldpi.png" gap:platform="android" gap:density="ldpi" />
    <icon src="icons/android/icon_mdpi.png" gap:platform="android" gap:density="mdpi" />
    <icon src="icons/android/icon_hdpi.png" gap:platform="android" gap:density="hdpi" />
    <icon src="icons/android/icon_xhdpi.png" gap:platform="android" gap:density="xhdpi" />


</widget>